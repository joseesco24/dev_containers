{
  "name": "golang dev container",
  "context": "../src",
  "dockerFile": "../development.Dockerfile",
  "forwardPorts": [],
  "workspaceMount": "source=${localWorkspaceFolder},target=/home/golang_dev/workspace,type=bind,consistency=delegated",
  "workspaceFolder": "/home/golang_dev/workspace",
  "postCreateCommand": "mkdir -p /home/golang_dev/workspace/bin && mkdir -p /home/golang_dev/workspace/pkg",
  "extensions": [
    "coenraads.bracket-pair-colorizer-2",
    "aaron-bond.better-comments",
    "alefragnani.bookmarks",
    "mhutchie.git-graph",
    "redhat.vscode-yaml",
    "ryu1kn.text-marker",
    "eamodio.gitlens",
    "golang.go",
  ]
}
